#include "Node.h"


Node::Node() : data(0), height(0), left(NULL), right(NULL) {}


int Node::getData()
{
	return this->data;
}



Node * Node::getLeftChild()
{
	return this->left;
}



Node * Node::getRightChild()
{
	return this->right;
}



int Node::getHeight()
{
	return this->height;
}

