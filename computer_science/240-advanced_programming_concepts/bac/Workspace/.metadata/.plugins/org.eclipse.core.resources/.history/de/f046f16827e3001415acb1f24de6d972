package Client.GUI.Entry;

import javax.swing.JPanel;
import javax.swing.JScrollPane;

@SuppressWarnings("serial")
public class TableEntryView extends JPanel
{
	public JScrollPane scrollPane;
	public TableEntryView()
	{
		scrollPane = new JScrollPane(this, JScrollPane.VERTICAL_SCROLLBAR_AS_NEEDED, JScrollPane.HORIZONTAL_SCROLLBAR_AS_NEEDED);
		System.out.println(this.getPreferredSize().getHeight());
		scrollPane.setPreferredSize(this.getPreferredSize());
	}
}

