package Client.GUI;

import java.awt.Color;

import javax.swing.JTabbedPane;

@SuppressWarnings("serial")
public class EntryView extends JTabbedPane
{
	public EntryView()
	{
		initUI();
	}
	
	private void initUI()
	{
		this.addTab("Table Entry", new TableEntryView());
		this.addTab("Form Entry", new FormEntryView());
	}

}
